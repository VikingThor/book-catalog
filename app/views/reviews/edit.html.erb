<h1>Edit Review</h1>

<%= form_for([@book, @review]) do |f| %>
 <div id="rating-form">
    <label>Rating </label>
  </div>
  <%= f.label :comment, "Comment" %>
  <%= f.text_field :comment %>
  <%= f.button :submit %>

<% end %>

<script>
  $('#rating-form').raty({
    path: '/assets/',
    scoreName: 'review[rating]'
  });
</script>